<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://sumgon.org/Article.xsd" xmlns:sumgon="http://sumgon.org/Article.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" id="Article" targetNamespace="http://sumgon.org/Article.xsd" elementFormDefault="qualified">

  <xs:annotation>
    <xs:appinfo>1.0.1</xs:appinfo>
    <xs:documentation>Version</xs:documentation>
    
  </xs:annotation>

  <xs:element name="Article">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="GlobalStyles" type="GlobalStylesType" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Title" type="ArticleTitleType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="Subtitle" type="ArticleSubtitleType" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Authors" type="AuthorsType" minOccurs="0" maxOccurs="1"/>
        <xs:choice minOccurs="1" maxOccurs="1">
          <xs:sequence>
            <xs:element name="Section1" type="Section1Type" minOccurs="1" maxOccurs="unbounded"/>
          </xs:sequence>
          <xs:choice minOccurs="1" maxOccurs="unbounded">
            <xs:element name="Paragraph" type="ParagraphType"/>
            <xs:element name="BlockImage" type="BlockImageType"/>
            <xs:element name="Code" type="CodeType"/>
            <xs:element name="BlockQuote" type="BlockQuoteType"/>
            <xs:element name="Video" type="VideoType"/>
            <xs:element name="Audio" type="AudioType"/>
            <xs:element name="OrderedList" type="OrderedListType"/>
            <xs:element name="UnorderedList" type="UnorderedListType"/>
            <xs:element name="Table" type="TableType"/>
          </xs:choice>
        </xs:choice>
        <xs:element name="References" type="ReferencesType" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="CiteKeyUnique">
      <xs:selector xpath="sumgon:Paragraph/sumgon:Cite"/>
      <xs:field xpath="@Id"/>
    </xs:unique>
    <xs:unique name="ReferenceKeyUnique">
      <xs:selector xpath="sumgon:References/sumgon:Reference"/>
      <xs:field xpath="@Id"/>
    </xs:unique>
    <xs:keyref name="ReferenceKeyReference" refer="ReferenceKey">
      <xs:selector xpath="sumgon:Paragraph/sumgon:Cite"/>
      <xs:field xpath="@ReferenceId"/>
    </xs:keyref>
    <xs:key name="ReferenceKey">
      <xs:selector xpath="sumgon:References/sumgon:Reference"/>
      <xs:field xpath="@Id"/>
    </xs:key>
  </xs:element>

  <!--Element Definition-->

  <!--Article Style Definition-->
  
  <xs:complexType name="GlobalStylesType">
    <xs:all>
      <xs:element name="TitleStyle" type="ArticleTitleStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="SubtitleStyle" type="ArticleSubtitleStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Section1HeadingStyle" type="SectionHeadingStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Section2HeadingStyle" type="SectionHeadingStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Section3HeadingStyle" type="SectionHeadingStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Section4HeadingStyle" type="SectionHeadingStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Section5HeadingStyle" type="SectionHeadingStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Section6HeadingStyle" type="SectionHeadingStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ParagraghStyle" type="ParagraphStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="BlockQuoteStyle" type="BlockQuoteStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="HyperlinkStyle" type="TextStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="MarkedTextStyle" type="TextStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="DeletedTextStyle" type="TextStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="SubscriptStyle" type="TextStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="SuperscriptStyle" type="TextStyleType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ReferenceStyle" type="TextStyleType" minOccurs="0" maxOccurs="1"/>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="ArticleTitleStyleType">
    <xs:attributeGroup ref="ArticleTitleAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="ArticleSubtitleStyleType">
    <xs:attributeGroup ref="ArticleSubtitleAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="AuthorsStyleType">
    <xs:attributeGroup ref="AuthorsAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="SectionHeadingStyleType">
    <xs:attributeGroup ref="SectionHeadingAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="ParagraphStyleType">
    <xs:attributeGroup ref="ParagraphAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="BlockQuoteStyleType">
    <xs:attributeGroup ref="BlockQuoteAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="TextStyleType">
    <xs:attributeGroup ref="TextAttributeGroup"/>
  </xs:complexType>

  <!--End Article Style Definition-->

  <!--Title and Subtitle-->

  <xs:complexType name="ArticleTitleType" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="StyledText" type="TextType"/>
      <xs:element name="LineBreak" type="LineBreakType"/>
    </xs:choice>
  </xs:complexType>

  <xs:complexType name="ArticleSubtitleType" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="StyledText" type="TextType"/>
      <xs:element name="LineBreak" type="LineBreakType"/>
    </xs:choice>
  </xs:complexType>

  <!--Abstract and Introduction-->

  <xs:complexType name="ArticleAbstractType">
    <xs:sequence>
      <xs:element name="Title" type="TextType" minOccurs="0" maxOccurs="1"/>
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element name="Paragraph" type="ParagraphType"/>
        <xs:element name="BlockImage" type="BlockImageType"/>
        <xs:element name="Code" type="CodeType"/>
        <xs:element name="BlockQuote" type="BlockQuoteType"/>
        <xs:element name="Video" type="VideoType"/>
        <xs:element name="Audio" type="AudioType"/>
        <xs:element name="OrderedList" type="OrderedListType"/>
        <xs:element name="UnorderedList" type="UnorderedListType"/>
        <xs:element name="Table" type="TableType"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ArticleIntroductionType">
    <xs:sequence>
      <xs:element name="Title" type="TextType" minOccurs="0" maxOccurs="1"/>
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element name="Paragraph" type="ParagraphType"/>
        <xs:element name="BlockImage" type="BlockImageType"/>
        <xs:element name="Code" type="CodeType"/>
        <xs:element name="BlockQuote" type="BlockQuoteType"/>
        <xs:element name="Video" type="VideoType"/>
        <xs:element name="Audio" type="AudioType"/>
        <xs:element name="OrderedList" type="OrderedListType"/>
        <xs:element name="UnorderedList" type="UnorderedListType"/>
        <xs:element name="Table" type="TableType"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <!--End Abstract and Introduction-->

  <!--Article Body-->

  <xs:complexType name="ArticleBodyType">
    <xs:choice>
      <xs:sequence>
        <xs:element name="Section1" type="Section1Type" minOccurs="1" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element name="Paragraph" type="ParagraphType"/>
        <xs:element name="BlockImage" type="BlockImageType"/>
        <xs:element name="Code" type="CodeType"/>
        <xs:element name="BlockQuote" type="BlockQuoteType"/>
        <xs:element name="Video" type="VideoType"/>
        <xs:element name="Audio" type="AudioType"/>
        <xs:element name="OrderedList" type="OrderedListType"/>
        <xs:element name="UnorderedList" type="UnorderedListType"/>
        <xs:element name="Table" type="TableType"/>
      </xs:choice>
    </xs:choice>

  </xs:complexType>

  <!--End Article Body-->

  <!--Section Type-->

  <xs:complexType name="Section1Type">
    <xs:sequence>
      <xs:element name="Heading" type="SectionHeadingType"/>
      <xs:choice>
        <xs:sequence>
          <xs:element name="Section2" type="Section2Type" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:choice minOccurs="1" maxOccurs="unbounded">
          <xs:element name="Paragraph" type="ParagraphType"/>
          <xs:element name="BlockImage" type="BlockImageType"/>
          <xs:element name="Code" type="CodeType"/>
          <xs:element name="BlockQuote" type="BlockQuoteType"/>
          <xs:element name="Video" type="VideoType"/>
          <xs:element name="Audio" type="AudioType"/>
          <xs:element name="OrderedList" type="OrderedListType"/>
          <xs:element name="UnorderedList" type="UnorderedListType"/>
          <xs:element name="Table" type="TableType"/>
        </xs:choice>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Section2Type">
    <xs:sequence>
      <xs:element name="Heading" type="SectionHeadingType"/>
      <xs:choice>
        <xs:sequence>
          <xs:element name="Section3" type="Section3Type" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:choice>
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element name="Paragraph" type="ParagraphType"/>
        <xs:element name="BlockImage" type="BlockImageType"/>
        <xs:element name="Code" type="CodeType"/>
        <xs:element name="BlockQuote" type="BlockQuoteType"/>
        <xs:element name="Video" type="VideoType"/>
        <xs:element name="Audio" type="AudioType"/>
        <xs:element name="OrderedList" type="OrderedListType"/>
        <xs:element name="UnorderedList" type="UnorderedListType"/>
        <xs:element name="Table" type="TableType"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Section3Type">
    <xs:sequence>
      <xs:element name="Heading" type="TextType"/>
      <xs:choice>
        <xs:sequence>
          <xs:element name="Section4" type="Section4Type" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:choice>
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element name="Paragraph" type="ParagraphType"/>
        <xs:element name="BlockImage" type="BlockImageType"/>
        <xs:element name="Code" type="CodeType"/>
        <xs:element name="BlockQuote" type="BlockQuoteType"/>
        <xs:element name="Video" type="VideoType"/>
        <xs:element name="Audio" type="AudioType"/>
        <xs:element name="OrderedList" type="OrderedListType"/>
        <xs:element name="UnorderedList" type="UnorderedListType"/>
        <xs:element name="Table" type="TableType"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Section4Type">
    <xs:sequence>
      <xs:element name="Heading" type="SectionHeadingType"/>
      <xs:choice>
        <xs:sequence>
          <xs:element name="Section5" type="Section5Type" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:choice minOccurs="1" maxOccurs="unbounded">
          <xs:element name="Paragraph" type="ParagraphType"/>
          <xs:element name="BlockImage" type="BlockImageType"/>
          <xs:element name="Code" type="CodeType"/>
          <xs:element name="BlockQuote" type="BlockQuoteType"/>
          <xs:element name="Video" type="VideoType"/>
          <xs:element name="Audio" type="AudioType"/>
          <xs:element name="OrderedList" type="OrderedListType"/>
          <xs:element name="UnorderedList" type="UnorderedListType"/>
          <xs:element name="Table" type="TableType"/>
        </xs:choice>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Section5Type">
    <xs:sequence>
      <xs:element name="Heading" type="SectionHeadingType"/>
      <xs:choice>
        <xs:sequence>
          <xs:element name="Section6" type="Section6Type" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:choice minOccurs="1" maxOccurs="unbounded">
          <xs:element name="Paragraph" type="ParagraphType"/>
          <xs:element name="BlockImage" type="BlockImageType"/>
          <xs:element name="Code" type="CodeType"/>
          <xs:element name="BlockQuote" type="BlockQuoteType"/>
          <xs:element name="Video" type="VideoType"/>
          <xs:element name="Audio" type="AudioType"/>
          <xs:element name="OrderedList" type="OrderedListType"/>
          <xs:element name="UnorderedList" type="UnorderedListType"/>
          <xs:element name="Table" type="TableType"/>
        </xs:choice>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Section6Type">
    <xs:sequence>
      <xs:element name="Heading" type="SectionHeadingType"/>
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element name="Paragraph" type="ParagraphType"/>
        <xs:element name="BlockImage" type="BlockImageType"/>
        <xs:element name="Code" type="CodeType"/>
        <xs:element name="BlockQuote" type="BlockQuoteType"/>
        <xs:element name="Video" type="VideoType"/>
        <xs:element name="Audio" type="AudioType"/>
        <xs:element name="OrderedList" type="OrderedListType"/>
        <xs:element name="UnorderedList" type="UnorderedListType"/>
        <xs:element name="Table" type="TableType"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="SectionHeadingType">
    <xs:simpleContent>
      <xs:extension base="xs:token">
        <xs:attributeGroup ref="SectionHeadingAttributeGroup"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <!--End Header Type-->

  <!--Paragraph Type-->
  <!--Block Element Cannot put into Paragraph-->
  <xs:complexType name="ParagraphType" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="StyledText" type="TextType"/>
      <xs:element name="DeletedText" type="DeletedTextType"/>
      <xs:element name="InsertedText" type="InsertedTextType"/>
      <xs:element name="MarkedText" type="MarkedTextType"/>
      <xs:element name="Superscript" type="SuperscriptType"/>
      <xs:element name="Subscript" type="SubscriptType"/>
      <xs:element name="Quote" type="QuoteType"/>
      <xs:element name="Cite" type="CiteType"/>
      <xs:element name="Hyperlink" type="HyperlinkType"/>
      <xs:element name="LineBreak" type="LineBreakType"/>
      <xs:element name="Space" type="SpaceType"/>
    </xs:choice>
    <xs:attributeGroup ref="ParagraphAttributeGroup"/>
  </xs:complexType>

  <!--End Paragraph Tyle-->

  <!--Author Type-->

  <xs:complexType name="AuthorsType">
    <xs:sequence>
      <xs:element name="Author" type="AuthorType"/>
    </xs:sequence>
    <xs:attribute name="HorizontalAlignment" type="HorizontalAlignmentType"/>
    <xs:attribute name="TopMargin" type="MarginType"/>
    <xs:attribute name="BottomMargin" type="MarginType"/>
  </xs:complexType>

  <xs:complexType name="AuthorType">
    <xs:simpleContent>
      <xs:extension base="TextType">
        <xs:attribute name="Source" type="xs:token"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <!--End Author Type-->

  <!--List Type-->

  <xs:complexType name="OrderedListType">
    <xs:sequence>
      <xs:element name="Item" type="ListItemType" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attributeGroup ref="OrderedListAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="UnorderedListType">
    <xs:sequence>
      <xs:element name="Item" type="ListItemType" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attributeGroup ref="UnorderedListAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="ListItemType">
    <xs:sequence>
      <xs:element name="Heading" type="TextType" minOccurs="1" maxOccurs="1"/>
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element name="Paragraph" type="ParagraphType"/>
        <xs:element name="OrderedList" type="OrderedListType"/>
        <xs:element name="UnorderedList" type="UnorderedListType"/>
        <xs:element name="BlockImage" type="BlockImageType"/>
        <xs:element name="Code" type="CodeType"/>
        <xs:element name="BlockQuote" type="BlockQuoteType"/>
        <xs:element name="Video" type="VideoType"/>
        <xs:element name="Audio" type="AudioType"/>
        <xs:element name="Table" type="TableType"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>

  <!--End List Type-->

  <!--Table Type-->

  <!--Width对列是有用的，只要设置了某一列上的一个单元格就会整列适用-->
  <!--Height对tr是有用的-->
  <!--设置单元格内部只能用padding，而且只能一个个单元格设置，但是会影响到列宽和行高-->

  <xs:complexType name="TableType">
    <xs:sequence>
      <xs:element name="Caption" type="TableCaptionType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ColumnStyles" type="TableColumnStylesType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Header" type="TableHeaderType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Body" type="TableBodyType" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attributeGroup ref="TableAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="TableColumnStylesType">
    <xs:sequence>
      <xs:element name="ColumnStyle" type="TableColumnStyleType" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="TableColumnStyleType">
    <xs:attributeGroup ref="TableColumnAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="TableHeaderType">
    <xs:sequence>
      <xs:element name="HeaderCell" type="TableHeaderCellType" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attributeGroup ref="TableHeaderAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="TableBodyType">
    <xs:sequence>
      <xs:element name="Row" type="TableRowType" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="RowHeight" type="HeightType"/>
  </xs:complexType>

  <xs:complexType name="TableRowType">
    <xs:choice minOccurs="1" maxOccurs="unbounded">
      <xs:element name="HeaderCell" type="TableBodyHeaderCellType"/>
      <xs:element name="DataCell" type="TableBodyDataCellType"/>
    </xs:choice>
    <xs:attributeGroup ref="TableRowAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="TableHeaderCellType" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="StyledText" type="TextType"/>
      <xs:element name="Hyperlink" type="HyperlinkType"/>
    </xs:choice>
    <xs:attributeGroup ref="TableHeaderCellAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="TableBodyHeaderCellType" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="StyledText" type="TextType"/>
      <xs:element name="Hyperlink" type="HyperlinkType"/>
    </xs:choice>
    <xs:attributeGroup ref="TableBodyCellAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="TableBodyDataCellType" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="StyledText" type="TextType"/>
      <xs:element name="Hyperlink" type="HyperlinkType"/>
    </xs:choice>
    <xs:attributeGroup ref="TableBodyCellAttributeGroup"/>
  </xs:complexType>
  
  <xs:complexType name="TableCaptionType">
    <xs:simpleContent>
      <xs:extension base="TextType">
        <xs:attribute name="CaptionSide" type="TableCaptionSideType"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <!--End Table Type-->

  <!--Reference and Cite Type-->

  <xs:complexType name="ReferencesType">
    <xs:sequence>
      <xs:element name="Heading" type="TextType" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Reference" type="ReferenceType" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attributeGroup ref="ReferencesAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="ReferenceType">
    <xs:simpleContent>
      <xs:extension base="TextType">
        <xs:attribute name="Id" type="xs:token" use="required"/>
        <xs:attribute name="Source" type="xs:token"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="CiteType">
    <xs:simpleContent>
      <xs:extension base="TextType">
        <xs:attribute name="Id" type="xs:token" use="required"/>
        <xs:attribute name="ReferenceId" type="xs:token" use="required"/>
        <xs:attribute name="ReferenceDescription" type="xs:token"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <!--End References and Cite Type-->
  
  <!--Block Image-->

  <xs:complexType name="BlockImageType">
    <xs:attributeGroup ref="BlockImageAttributeGroup"/>
  </xs:complexType>
   
  <!--End Block Image-->
  
  <!--Other Block Element-->

  <xs:complexType name="CodeType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attributeGroup ref="CodeAttributeGroup"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="BlockQuoteType">
    <xs:sequence>
      <xs:element name="Paragraph" type="ParagraphType" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attributeGroup ref="BlockQuoteAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="VideoType">
    <xs:attributeGroup ref="VideoAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="AudioType">
    <xs:attributeGroup ref="AudioAttributeGroup"/>
  </xs:complexType>

  <xs:complexType name="BlockTextType">
    <xs:simpleContent>
      <xs:extension base="xs:token">
        <xs:attributeGroup ref="BlockTextAttributeGroup"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  
  <!--End Other Block Element-->

  <!--Other Inline Element Type-->

  <xs:complexType name="TextType">
    <xs:simpleContent>
      <xs:extension base="xs:token">
        <xs:attributeGroup ref="TextAttributeGroup"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="HyperlinkType">
    <xs:simpleContent>
      <xs:extension base="TextType">
        <xs:attribute name="Source" type="xs:token" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="LineBreakType"/>

  <xs:complexType name="SpaceType"/>

  <xs:complexType name="DeletedTextType">
    <xs:simpleContent>
      <xs:restriction base="TextType"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="InsertedTextType">
    <xs:simpleContent>
      <xs:restriction base="TextType"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="MarkedTextType">
    <xs:simpleContent>
      <xs:restriction base="TextType"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="SuperscriptType">
    <xs:simpleContent>
      <xs:restriction base="TextType"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="SubscriptType">
    <xs:simpleContent>
      <xs:restriction base="TextType"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="QuoteType">
    <xs:simpleContent>
      <xs:extension base="TextType">
        <xs:attribute name="Source" type="xs:token"/>
        <xs:attribute name="QuoteStyle" type="QuoteStyleType"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="ImageType">
    <xs:attributeGroup ref="ImageAttributeGroup"/>
  </xs:complexType>
  
  <!--End Base Element Type-->
  
  <!--Attribute Group Type-->
  
  <!--因为html居中需要用margin-left/margin-left:auto和width:50%，所以左右margin无法使用（默认的margin会远比设置的大），需要使用width:number%来进行左右对称margin-->

  <xs:attributeGroup name="ArticleTitleAttributeGroup">
    <xs:attribute name="TextAlignment" type="TextAlignmentType"/>
    <xs:attribute name="FontSize" type="FontSizeType"/>
    <xs:attribute name="FontFamily" type="FontFamilyType"/>
    <xs:attribute name="FontStretch" type="FontStretchType"/>
    <xs:attribute name="FontStyle" type="FontStyleType"/>
    <xs:attribute name="FontWeight" type="FontWeightType"/>
    <xs:attribute name="UnderlineStyle" type="UnderlineStyleType"/>
